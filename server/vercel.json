{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "./",
  "regions": ["iad1"],
  "functions": {
    "server.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.ts"
    }
  ]
}
